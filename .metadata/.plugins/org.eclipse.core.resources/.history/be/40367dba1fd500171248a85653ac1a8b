
function changeSelector(selector, up, down, right, left){
	var num = 0;
	
	for ( var i = 1; i < 24; i++){

		if( selector.name == i){
			
			if( up == 1){
				if( selector.y != 1){
					num = parseInt(selector.name)-6;
					var aux = document.getElementsByName(num);
					aux[0].id = 'selector';
					selector.id = "";
				}
			}
			if( down == 1){
				if( selector.y != 4){
					num = parseInt(selector.name)+6;
					var aux = document.getElementsByName(num);
					aux[0].id = 'selector';
					selector.id = "";
				}
				
			}
			if( right == 1){
				if( selector.x != 6){
					num = parseInt(selector.name)+6;
					var aux = document.getElementsByName(num);
					aux[0].id = 'selector';
					selector.id = "";
				}
			}
			if( left == 1){
				
			}
		}
	}
	
	
	
	
	
}