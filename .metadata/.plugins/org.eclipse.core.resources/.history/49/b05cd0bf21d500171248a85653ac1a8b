
function changeSelector(selector, up, down, right, left){
	var num = 0;
	
	for ( var i = 1; i < 25; i++){

		if( selector.name == i){
			console.log(selector.x);
			if( up == 1){
				num = parseInt(selector.name)-6;
				if( num  > 1){
					var aux = document.getElementsByName(num);
					aux[0].id = 'selector';
					selector.id = "";
				}
			}
			if( down == 1){
				num = parseInt(selector.name)-6;
				if( num  < 24){
					
					var aux = document.getElementsByName(num);
					aux[0].id = 'selector';
					selector.id = "";
				}
				
			}
			if( right == 1){
				num = parseInt(selector.name)+1;
				if(num < 25 ){
					var aux = document.getElementsByName(num);
					aux[0].id = 'selector';
					selector.id = "";
				}
		
			}
			if( left == 1){
				num = parseInt(selector.name)-1;
				if(num > 1 ){
					var aux = document.getElementsByName(num);
					aux[0].id = 'selector';
					selector.id = "";
				}
			}
		}
	}
}